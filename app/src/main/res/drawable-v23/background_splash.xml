<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
            android:drawable="@color/colorPrimary"/>

    <item>
        <bitmap
                android:gravity="center"
                android:src="@drawable/splash"/>

    </item>

    <item
            android:gravity="left"
            android:top="1dp"
            android:left="5dp"
            android:drawable="@drawable/logoupn"
            android:height="10dp"
            android:width="10dp">

    </item>

    <item
            android:gravity="right"
            android:top="1dp"
            android:right="5dp"
            android:height="10dp"
            android:drawable="@drawable/logo_si"
            android:width="10dp">

    </item>




</layer-list>